<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bit.mall.dao.ProductDao">
    <select id="selectByPrimaryKey" resultType="com.bit.model.Product">
        select id, `name`, imgs from product where id = #{id}
    </select>

    <select id="query" resultType="com.bit.model.Product">
        select id, `name`, imgs from product
    </select>
</mapper>